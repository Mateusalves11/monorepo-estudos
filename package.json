{
    "private": true,
    "workspaces": {
        "packages": [
            "packages/*",
            "projects/*",
            "setup/*"
        ]
    },
    "scripts": {
        "lint": "yarn workspaces run lint",
        "web-public": "yarn workspace @gw/web-public",
        "dev:web-public": "yarn web-public dev",
        "build:web-public": "yarn web-public build",
        "start:web-public": "yarn web-public start",
        "lint:web-public": "yarn web-public lint",
        "design-system": "yarn workspace @gw/design-system",
        "utils": "yarn workspace @gw/utils",
        "test:utils": "yarn utils test"
    },
    "dependencies": {
        "eslint": "8.38.0",
        "prettier": "2.8.7",
        "@types/styled-components": "5.1.26",
        "@types/node": "^18.15.3",
        "@types/jest": "^27.5.0",
        "@types/react": "^18.0.28",
        "typescript": "4.9.5"
    }
}